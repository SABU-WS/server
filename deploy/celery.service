[Unit]
Description=CELERY-Worker
After=network.target
StartLimitInterval=200
StartLimitBurst=5

[Service]
User=svc-sabu
Group=svc-sabu
WorkingDirectory=/sabu/server/
ExecStart=/sabu/venv-sabu/bin/celery -A app.scanner worker --loglevel=info
StandardOutput=append:/sabu/logs/server/celery_output.log
StandardError=append:/sabu/logs/server/celery_error.log
Restart=always
RestartSec=30

[Install]
WantedBy=multi-user.target
